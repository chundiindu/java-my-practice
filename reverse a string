import java.util.*;
public class MyClass {
    public static void main(String args[]) {
        
        String str= "hello iam indu";
     Stack<Character> st = new Stack<>();
     for(int i=0;i<str.length();i++)
     {
         st.push(str.charAt(i));
     }
     while(!st.isEmpty())
     {
     System.out.print(st.pop());
    }
}
}
